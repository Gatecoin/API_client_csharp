<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>GatecoinServiceInterface</RootNamespace>

    
    <Description>Gatecoin.com API .Net client</Description>
