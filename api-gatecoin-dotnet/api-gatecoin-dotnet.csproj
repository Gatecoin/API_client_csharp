    <Copyright>Copyright Â©Gatecoin</Copyright>
    <PackageIconUrl>https://github.com/Gatecoin/api-gatecoin-dotnet/blob/master/logo.png</PackageIconUrl>
    <PackageProjectUrl>https://gatecoin.com</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Gatecoin/api-gatecoin-dotnet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseUrl>https://github.com/Gatecoin/api-gatecoin-dotnet/blob/master/LICENSE</PackageLicenseUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
